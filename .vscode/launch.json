    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: FastAPI",
            "type": "debugpy",
            "request": "launch",
            "args": ["--reload"],
            "program": "${workspaceFolder}/backend/main.py",
            "jinja": true,
            "cwd": "${workspaceFolder}/backend",
        },
        {
            "name": "Python Debugger: Bot",
            "type": "debugpy",
            "request": "launch",
            "args": ["--reload"],
            "program": "${workspaceFolder}/bot/main.py",
            "jinja": true,
            "cwd": "${workspaceFolder}/bot",
        }
    ]
}